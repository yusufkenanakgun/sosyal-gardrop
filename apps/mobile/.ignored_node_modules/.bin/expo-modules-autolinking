#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Yusuf/Desktop/sosyal-gardrop/node_modules/.pnpm/expo@52.0.47_@babel+core@7._dbb7e560574638ec6eb399c8d4bc0e7f/node_modules/expo/bin/node_modules:/mnt/c/Users/Yusuf/Desktop/sosyal-gardrop/node_modules/.pnpm/expo@52.0.47_@babel+core@7._dbb7e560574638ec6eb399c8d4bc0e7f/node_modules/expo/node_modules:/mnt/c/Users/Yusuf/Desktop/sosyal-gardrop/node_modules/.pnpm/expo@52.0.47_@babel+core@7._dbb7e560574638ec6eb399c8d4bc0e7f/node_modules:/mnt/c/Users/Yusuf/Desktop/sosyal-gardrop/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Yusuf/Desktop/sosyal-gardrop/node_modules/.pnpm/expo@52.0.47_@babel+core@7._dbb7e560574638ec6eb399c8d4bc0e7f/node_modules/expo/bin/node_modules:/mnt/c/Users/Yusuf/Desktop/sosyal-gardrop/node_modules/.pnpm/expo@52.0.47_@babel+core@7._dbb7e560574638ec6eb399c8d4bc0e7f/node_modules/expo/node_modules:/mnt/c/Users/Yusuf/Desktop/sosyal-gardrop/node_modules/.pnpm/expo@52.0.47_@babel+core@7._dbb7e560574638ec6eb399c8d4bc0e7f/node_modules:/mnt/c/Users/Yusuf/Desktop/sosyal-gardrop/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../expo/bin/autolinking" "$@"
else
  exec node  "$basedir/../expo/bin/autolinking" "$@"
fi
